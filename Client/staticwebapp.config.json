{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/api/*" ]
  },
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/**/*.dll",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/**/*.wasm",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/**/*.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/**/*.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
