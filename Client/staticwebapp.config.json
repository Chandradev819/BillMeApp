{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/api/hello/protected/admin",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/api/hello/protected/superadmin",
      "allowedRoles": [ "authenticated" ]
    }
  ]
}
